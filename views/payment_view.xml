<?xml version="1.0" encoding="utf-8" ?>
<openerp>
    <data>

        <record id="payment_tree_view" model="ir.ui.view">
            <field name="model">payment main tree view</field>
            <field name="model">travel.payment</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="travel_id" invisible="1"/>
                    <field name="amount"/>
                    <field name="currency_id"/>
                    <field name="company_currency_amount"/>
                    <field name="company_currency_id"/>
                    <field name="payment_date"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>